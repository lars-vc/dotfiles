{
        "configurations": {
                "<name>: Launch": {
                        "adapter": "debugpy",
                        "configuration": {
                                "name": "<name>: Launch",
                                "type": "python",
                                "request": "launch",
                                "cwd": "${workspaceRoot}",
                                "python": "/usr/bin/python3",
                                "stopOnEntry": true,
                                "console": "externalTerminal",
                                "debugOptions": [],
                                "program": "${file}"
                        }
                }
        }
}
